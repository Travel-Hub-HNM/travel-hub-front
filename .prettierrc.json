{
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 120,
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "bracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
}
